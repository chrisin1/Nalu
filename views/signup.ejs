    <form class = "login-form" id="signup" name="signup" method="post" action="/signup">

      <div class = "username-row row">
          <div class="form-group">
            <label for="firstname"></label>
            <input name="firstname" type="text" placeholder="First Name"/>
          </div>
      </div>



      <div class = "username-row row">
          <div class="form-group">
            <label for="lastname"></label>
            <input name="lastname" type="text" placeholder="Last Name"/>
          </div>
      </div>

      <div class = "username-row row">
          <div class="form-group">
            <label for="username"></label>
            <input name="username" type="text" placeholder="User Name"/>
          </div>
      </div>

      <div class = "username-row row">
          <div class="form-group">
            <label for="email"></label>
            <input class="text form-control" name="email" type="email" placeholder="Email">
          </div>
      </div>

      <div class = "password-row row">
          <div class="form-group">
            <label for="password"></label>
            <input type="password" name="password" class="form-control" placeholder="Password">
          </div>
      </div>
          <center>
            <button type="submit" id = "button" class="btn btn-warning btn-lg">Create Account</button>
          </center>
    </form>



    <div class="call-to-action">


